----
NULL

query T
SELECT CASE WHEN 24 + 1 = 26 - 1 THEN NULL ELSE 24 + 1 END
----
NULL

query T
SELECT NULLIF(1, 1) IS NULL;
----
true

query T
SELECT CASE WHEN 1 = 1 THEN NULL ELSE 1 END IS NULL;
----
true

statement ok
CREATE TABLE TBL_WITH_NULLS(ID INT PRIMARY KEY, VAL VARCHAR, NUM INT)

statement ok
INSERT INTO TBL_WITH_NULLS VALUES (0, 'abc', 0), (1, 'abc', NULL), (2, NULL, 0)

query T
select case when (VAL = 'abc') then 0 else (case when (NUM = 0) then 1 else NULL end) end IS NULL from TBL_WITH_NULLS
----
false
false
false

# NULL in THEN operand.
query T
select case when (VAL = 'diff') then 0 else (case when (NUM IS NULL) then NULL else 1 end) end IS NULL from TBL_WITH_NULLS order by ID
----
false
true
false

# NULL in ELSE operand.
query T
select case when (VAL = 'diff') then 0 else (case when (NUM IS NULL) then 1 else NULL end) end IS NULL from TBL_WITH_NULLS order by ID
----
true
false
true

query T
select case when (VAL is NULL) then '0' else (case when (NUM IS NULL) then '1' else NULL end) end IS NULL from TBL_WITH_NULLS order by ID
----
true
false
false

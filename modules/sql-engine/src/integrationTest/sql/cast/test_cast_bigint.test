# name: test/sql/cast/test_cast_bigint.test
# description: CAST BIGINT
# feature: SQL F201(CAST function)
# group: [cast]

statement ok
SELECT CAST('100' AS BIGINT);

# https://issues.apache.org/jira/browse/IGNITE-22519
skipif ignite3
statement error
SELECT CAST('100.1' AS BIGINT);

query T
SELECT CAST(1e1 AS BIGINT);
----
10

# https://issues.apache.org/jira/browse/IGNITE-22519
skipif ignite3
statement error: For input string: "1e2"
SELECT CAST('1e2' AS BIGINT);

# overflow
statement error
SELECT CAST(1e40 AS BIGINT);

# overflow
statement error
SELECT CAST(-1e40 AS BIGINT);

query T
SELECT CAST(100::TINYINT AS BIGINT);
----
100

query T
SELECT CAST(100::SMALLINT AS BIGINT);
----
100

query T
SELECT CAST(100::INT AS BIGINT);
----
100

query T
SELECT CAST(100::DECIMAL AS BIGINT);
----
100

query T
SELECT CAST(100::DECIMAL(3) AS BIGINT);
----
100

query T
SELECT CAST(100.1::DECIMAL(4, 1) AS BIGINT);
----
100

query T
SELECT CAST(100.1::REAL AS BIGINT);
----
100

query T
SELECT CAST(100.1::FLOAT AS BIGINT);
----
100

query T
SELECT CAST(100.1::DOUBLE AS BIGINT);
----

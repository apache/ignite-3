# name: test/sql/cast/test_cast_real.test
# description: CAST REAL
# feature: SQL F201(CAST function)
# group: [cast]

statement ok
SELECT CAST('100.0' AS REAL);

query T
SELECT CAST('100' AS REAL);
----
100.0

# https://issues.apache.org/jira/browse/IGNITE-22519
# overflow
skipif ignite3
statement error
SELECT CAST(1e39 AS REAL);

# https://issues.apache.org/jira/browse/IGNITE-22519
# overflow
skipif ignite3
statement error
SELECT CAST(-1e39 AS REAL);

query T
SELECT CAST(1e1 AS REAL);
----
10

query I
SELECT CAST('1e1' AS REAL);
----
10.0

query I
SELECT CAST('   1e1' AS REAL);
----
10.0


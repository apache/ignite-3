{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "Apache.Ignite/bin/Debug/net6.0/Apache.Ignite.dll"
                    ],
                    "src": "../"
                }
            ],
            "dest": "api",
            "includePrivateMembers": false,
            "disableGitFeatures": false,
            "disableDefaultFilter": false,
            "noRestore": false,
            "namespaceLayout": "flattened",
            "memberLayout": "samePage",
            "allowCompilationErrors": false
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "articles/**.md",
                    "articles/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "modern"
        ],
        "postProcessors": [],
        "keepFileLink": false,
        "disableGitFeatures": false
    }
}
